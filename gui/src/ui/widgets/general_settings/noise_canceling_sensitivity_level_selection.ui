<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkAdjustment" id="noise-canceling-sensitivity-level-adjustment">
        <property name="lower">0</property>
        <property name="upper">10</property>
        <property name="step-increment">1</property>
        <property name="page-increment">1</property>
    </object>
    <template class="OpenSCQ30NoiseCancelingSensitivityLevelSelection" parent="GtkBox">
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkLabel" id="label">
                <property name="label" translatable="yes">Noise Canceling Sensitivity Level</property>
                <style>
                    <class name="general-settings__heading-small" />
                </style>
            </object>
        </child>
        <child>
            <object class="GtkScale" id="scale">
                <property name="hexpand">true</property>
                <property name="adjustment">noise-canceling-sensitivity-level-adjustment</property>
                <property name="digits">0</property>
                <property name="draw-value">true</property>
                <property name="round-digits">0</property>
                <accessibility>
                    <relation name="GTK_ACCESSIBLE_RELATION_LABELLED_BY">label</relation>
                </accessibility>
            </object>
        </child>
    </template>
</interface>
